{
	"dimension": {
		"filename": "dimension_eideticker.json"
	},
	"param": {
		"default": {
			"select": {
				"source_ref": {
					"name": "uuid",
					"value": "metadata.uuid"
				},
				"repo": "Eideticker.Revision",
				"value": {
					"name": "value",
					"value": "metrics.checkerboard"
				}
			},
			"sort": {
				"name": "push_date",
				"value": "metadata.date"
			},
			"units": "unknown",
			"better": "lower",
			"min_regression": "5%",
			"min_mscore": 2.0,
			"min_tscore": 4.0,
			"window_size": 20
		},
		"branch": {
		},
		"test": {
			"nytimes-load": {
				"select": {
					"value": {
						"value": "metrics.timetostableframe"
					}
				}
			},
			"nytimes-load-poststartup": {
				"select": {
					"value": {
						"value": "metrics.timetostableframe"
					}
				}
			},
			"startup-abouthome-dirty": {
				"select": {
					"value": {
						"value": "metrics.timetostableframe"
					}
				}
			},
			"startup-abouthome-fresh": {
				"select": {
					"value": {
						"value": "metrics.timetostableframe"
					}
				}
			}
		},
		"reason": "eideticker_alert_sustained_median",  //ADDED TO ALERT FOR MARKUP OF ALERT TYPE
		"severity": 0.8,
		"test_dimension": "Eideticker.Test",
		"revision_dimension": "Eideticker.Revision",
		"branch_dimension": "Eideticker.Branch",
		"debug": true,
		"mark_complete": "kyle_test_eideticker_sustained_median",
		"combo_limit": 50  //SO WE DO NOT DO TOO MUCH WORK EACH TIME (BE SURE TO MULTIPLY BY NUMBER OF TESTS IN A SUITE)
	},
	"query": {
		"from": {
			"name": "eideticker",
			"host": "http://localhost",
			"port": 9200,
			"index": "eideticker",
			"type": "results",
			"debug": true
		},
		"edges": [
			"Eideticker.Device",
			"Eideticker.Branch",
			"Eideticker.Test"
		]
	},
	"alerts": {
		"host": "localhost",
		"port": 3306,
		"username": "username",
		"password": "password",
		"schema": "alerts",
		"debug": true
	},
	"debug": {
		"trace": true,
		"log": [
			{
				"class": "logging.handlers.RotatingFileHandler",
				"filename": "results/logs/eideticker_import.log",
				"maxBytes": 10000000,
				"backupCount": 100,
				"encoding": "utf8"
			},
			{
				"log_type": "stream",
				"stream": "sys.stdout"
			}
		]
	}
}
