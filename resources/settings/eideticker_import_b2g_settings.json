{
	"param": {
		"url": "http://eideticker.mozilla.org/b2g/",
		"uuid_url": "http://eideticker.mozilla.org/b2g/metadata/{{uuid}}.json",
		"elasticsearch": {
			"name": "eideticker",
			"host": "http://localhost",
			"port": 9200,
			"index": "eideticker",
			"type": "results",
			"schema_file": "resources/settings/eideticker_schema.json",
			"debug": true,
			"limit_replicas": true
		}
	},
	"debug": {
		"trace": true,
		"log": [
			{
				"class": "logging.handlers.RotatingFileHandler",
				"filename": "results/logs/eideticker_import_b2g.log",
				"maxBytes": 10000000,
				"backupCount": 100,
				"encoding": "utf8"
			},
			{
				"log_type": "stream",
				"stream": "sys.stdout"
			}
		]
	}
}
