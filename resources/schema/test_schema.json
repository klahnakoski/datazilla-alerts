{
	"settings": {
		"index.store.throttle.type": "merge",
		"index.number_of_replicas": 0,
		"index.store.throttle.max_bytes_per_sec": "2mb",
		"index.number_of_shards": 3
	},
	"mappings": {
		"test_results": {
			"_source": {
				"compress": true
			},
			"dynamic_templates": [
				{
					"xperf": {
						"mapping": {
							"index": "not_analyzed",
							"type": "string"
						},
						"match": "results_xperf.*"
					}
				},
				{
					"default_strings": {
						"mapping": {
							"index": "not_analyzed",
							"type": "string"
						},
						"match_mapping_type": "string",
						"match": "*"
					}
				}
			],
			"properties": {
				"talos_aux": {
					"enabled": false,
					"type": "object",
					"index": "no",
					"store": "yes"
				},
				"results_xperf": {
					"enabled": false,
					"type": "object",
					"index": "no",
					"store": "yes"
				},
				"result": {
					"properties": {
						"stats": {
							"dynamic": true,
							"properties": {
								"min": {
									"type": "double"
								},
								"s0": {
									"type": "long"
								},
								"s2": {
									"type": "double"
								},
								"kurtosis": {
									"type": "double"
								},
								"max": {
									"type": "double"
								},
								"s1": {
									"type": "double"
								},
								"count": {
									"type": "long"
								},
								"last": {
									"type": "double"
								},
								"std": {
									"type": "double"
								},
								"variance": {
									"type": "double"
								},
								"unbiased": {
									"type": "boolean"
								},
								"median": {
									"type": "double"
								},
								"mean": {
									"type": "double"
								},
								"first": {
									"type": "double"
								},
								"skew": {
									"type": "double"
								},
								"s3": {
									"type": "double"
								},
								"s4": {
									"type": "double"
								}
							}
						},
						"test_name": {
							"index": "not_analyzed",
							"type": "string"
						},
						"ordering": {
							"type": "integer"
						},
						"samples": {
							"type": "double"
						}
					}
				},
				"datazilla": {
					"properties": {
						"id": {
							"type": "long"
						},
						"date_loaded": {
							"type": "long"
						},
						"test_run_id": {
							"type": "long"
						},
						"error_flag": {
							"index": "not_analyzed",
							"type": "string"
						},
						"processed_flag": {
							"index": "not_analyzed",
							"type": "string"
						},
						"error_msg": {
							"index": "not_analyzed",
							"type": "string"
						}
					}
				},
				"run": {
					"properties": {
						"suite": {
							"index": "not_analyzed",
							"norms": {
								"enabled": false
							},
							"index_options": "docs",
							"type": "string"
						},
						"date": {
							"type": "long"
						},
						"options": {
							"dynamic": true,
							"properties": {
								"tprender": {
									"type": "boolean"
								},
								"responsiveness": {
									"type": "boolean"
								},
								"tploadaboutblank": {
									"type": "boolean"
								},
								"tpdelay": {
									"type": "long"
								},
								"tpmozafterpaint": {
									"type": "boolean"
								},
								"tpcycles": {
									"type": "long"
								},
								"tppagecycles": {
									"type": "long"
								},
								"tpchrome": {
									"type": "boolean"
								},
								"rss": {
									"type": "boolean"
								},
								"extensions": {
									"dynamic": true,
									"properties": {
										"name": {
											"index": "not_analyzed",
											"norms": {
												"enabled": false
											},
											"index_options": "docs",
											"type": "string"
										}
									}
								},
								"shutdown": {
									"type": "boolean"
								}
							}
						}
					}
				},
				"machine": {
					"properties": {
						"platform": {
							"index": "not_analyzed",
							"type": "string"
						},
						"os": {
							"index": "not_analyzed",
							"type": "string"
						},
						"name": {
							"index": "not_analyzed",
							"type": "string"
						},
						"osversion": {
							"index": "not_analyzed",
							"type": "string"
						}
					}
				},
				"treeherder": {
					"properties": {
						"branch": {
							"index": "not_analyzed",
							"type": "string"
						},
						"perf_id": {
							"index": "not_analyzed",
							"type": "long"
						},
						"url": {
							"index": "not_analyzed",
							"type": "string"
						},
						"reason": {
							"index": "not_analyzed",
							"type": "string"
						},
						"corrupt_json": {
							"type": "string",
							"enabled": false,
							"index": "no",
							"store": "yes"
						}
					}
				},
				"results_aux": {
					"enabled": false,
					"type": "object",
					"index": "no",
					"store": "yes"
				},
				"build": {
					"properties": {
						"id": {
							"type": "long"
						},
						"revision": {
							"index": "not_analyzed",
							"type": "string"
						},
						"no_pushlog": {
							"type": "boolean"
						},
						"pgo": {
							"type": "boolean"
						},
						"name": {
							"index": "not_analyzed",
							"type": "string"
						},
						"push_date": {
							"type": "double"
						},
						"branch": {
							"index": "not_analyzed",
							"type": "string"
						},
						"version": {
							"index": "not_analyzed",
							"type": "string"
						}
					}
				}
			},
			"_all": {
				"enabled": false
			}
		}
	}

}
