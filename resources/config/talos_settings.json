{
	"dimension": {
		"$ref": "file://dimension_talos.json"
	},
	"param": {
		"default": {
			"select": {
				"repo": "Talos.Revision",
				"value": {
					"name": "value",
					"value": "result.stats.median"
				}
			},
			"sort": {
				"name": "push_date",
				"value": "build.push_date"
			},
			"units": "unknown",
			"better": "lower",
			"min_regression": "5%",
			"min_mscore": 2.5,
			"min_tscore": 4.0,
			"window_size": 20
		},
		"branch": {
			"Try": {
				"disable": true
			},
			"Try-Non-PGO": {
				"disable": true
			},
			"Cedar": {
				"disable": true
			},
			"Cedar-Non-PGO": {
				"disable": true
			},
			"Ash": {
				"disable": true
			},
			"Ash-Non-PGO": {
				"disable": true
			}
		},
		"suite": {
			"dromaeo_css": {
				"window_size": 50,
				"min_regression": "15%",
				"better": "higher"
			},
			"dromaeo_dom": {
				"window_size": 30,
				"min_regression": "15%",
				"better": "higher"
			},
			"tp5o_scroll": {
				"window_size": 30,
				"min_regression": "15%"
			},
			"v8_7": {
				"better": "higher"
			},
			"kraken": {
				"better": "higher"
			},
			"canvasmark": {
				"better": "higher"
			},
			"tp5o": {
				"units": "ms",
				"better": "lower",
				"min_regression": "15%"
			},
			"tp4m": {
				"units": "ms",
				"select": {
					"value": {
						"name": "value",
						"value": "result.stats.last"
					}
				}
			}
		},
		"test": {
			"_dromaeo_css_summary": {
				"window_size": 30,
				"min_regression": "3%",
				"better": "higher"
			},
			"_dromaeo_dom_summary": {
				"window_size": 30,
				"min_regression": "3%",
				"better": "higher"
			},
			"_tp5o_summary": {
				"better": "lower",
				"min_regression": "3%"
			},
			"1-customize-enter.error.TART": {
				"units": "unknown units",
				"better": "lower",
				"min_regression": "1%"
			},
			"tablemutation.html": {
				"units": "unknown units",
				"better": "lower",
				"min_regression": "3%"
			},
			"alipay.com": {
				//VERY NOISY TEST
				"window_size": 50,
				"select": {
					"value": {
						"name": "value",
						"value": "result.stats.min"
					}
				}
			},
			"SUMMARY":{
				"units": "geometric mean",
				"min_regression": "3%"
			}
		},
		"reason": "talos_alert_sustained_median", //ADDED TO ALERT FOR MARKUP OF ALERT TYPE
		"severity": 0.8,
		"test_dimension": "Talos.Test",
		"revision_dimension": "Talos.Revision",
		"branch_dimension": "Talos.Branch",
		"source_ref": [
			"Talos.Test",
			{"name": "test_run_id", "value": "treeherder.perf_id"}
		],
		"verbose": true,
		"mark_complete": {"$ref":"file://~/private_dzalerts.json#mark_complete",
		"combo_limit": 50, // SO WE DO NOT DO TOO MUCH WORK EACH TIME (BE SURE TO MULTIPLY BY NUMBER OF TESTS IN A SUITE)
		"debug": true,    // SET TO true TO INJECT debug_filters INTO THE QUERY STREAM AND FOCUS ON THOSE
		"debug_filter": {
			"or": [
				{
					"and": [
//						{"term": {"run.suite": "tp5o"}},
						{"term": {"result.test_name": "tpaint"}},
						{"term": {"machine.osversion": "6.1.7601"}},
						{"term": {"build.branch": "Fx-Team-Non-PGO"}}
					]
				}
			]
		}
	},
	"query": {
		"from": {
			"name": "talos",
			"host": "http://localhost",
			"port": 9200,
			"index": "talos",
			"type": "test_results",
			"debug": true
		},
		"edges": [
			"Talos.Product",
			"Talos.Branch",
			"Talos.OS",
			"Talos.Platform",
			"Talos.Test",
			"Talos.Build_Options",
			"Talos.e10s"
		]
	},
	"alerts": {
		"$ref": "file://~/private_dzalerts.json#alerts_db"
	},
	"pushlog": {
		"schema": "pushlog_hgmozilla_1"
	},
	"email": {
		"from_address": "klahnakoski@mozilla.com",
		"$ref": "file://~/private_dzalerts.json#email"
	},
	"constants": {
		"pyLibrary.env.http.default_headers":{
			"from":"https://wiki.mozilla.org/Auto-tools/Projects/Alerts"
		}
	},
	"debug": {
		"trace": false,
		"log": [
			{
				"class": "logging.handlers.RotatingFileHandler",
				"filename": "results/logs/talos_alert.log",
				"maxBytes": 10000000,
				"backupCount": 100,
				"encoding": "utf8"
			},
			{
				"log_type": "stream",
				"stream": "sys.stdout"
			}
		]
	}
}

